<%- include ("header") -%>

<h1> <%= listTitle %> </h1>

<ul>
  <% for (i = 0; i < newListItem.length; i++) {%>
  <li> <%= newListItem[i] %> </li>
  <% } %>
</ul>

<form action="/" method="post">
  <input type="text" name="newItem">
  <button type="submit" name="list" value=<%= listTitle %>>Add new item</button>
</form>

<%- include ("footer") -%>